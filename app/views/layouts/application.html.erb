<!DOCTYPE html>
<html>
<head>
  <title>Getsy</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>

  <header class="group">
    <%= render partial: "layouts/header_search" %>
    <% if signed_in? %>
      <%= render partial: "layouts/header_nav_signed_in" %>
    <% else %>
      <%= render partial: "layouts/header_nav" %>
    <% end %>
  </header>
  <div id="main-content">
    <%= yield %>
  </div>



  <footer class="site-footer">
    <%= render partial: "layouts/footer" %>
  </footer>
</body>
</html>
