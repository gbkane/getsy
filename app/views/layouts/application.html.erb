<!DOCTYPE html>
<html>
<head>
  <title>Getsy</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <%= render partial: "layouts/header_search" %>
    <% if signed_in? %>
      <%= render partial: "layouts/header_nav_signed_in" %>
    <% else %>
      <%= render partial: "layouts/header_nav" %>
  </header>

  <%= yield %>
</body>
</html>
